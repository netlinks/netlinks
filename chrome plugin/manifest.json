{
  "manifest_version": 2,

  "name": "NetLinks",
  "description": "Save your favourite links online",
  "version": "1.1.1.1",
  "icons": { "48": "drive-cloud.png"},
 
  "browser_action": {
	"default_icon": "drive-cloud.png",
	"default_popup": "popup.html" 
  },
  
  "permissions": [
    "tabs","contextMenus","bookmarks",
    "https://*/*","http://*/*"
  ]
}