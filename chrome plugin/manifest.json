{
  "manifest_version": 2,

  "name": "Netlinks",
  "description": "Netlinks plugin for adding your favorite links to Netlinks application",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.jpg",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs","contextMenus",
    "https://*/*","http://*/*"
  ],
  "content_scripts": [
  {
    "matches": ["http://*/*","https://*/*"],
    "js": ["jquery.js"],
    "run_at": "document_start",
    "all_frames": true
  }]
}