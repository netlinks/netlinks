{
  "manifest_version": 2,

  "name": "Save Link",
  "description": "Netlinks plugin for adding your favorite links to Netlinks application",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.ico",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs","contextMenus","bookmarks",
    "https://*/*","http://*/*"
  ]
}